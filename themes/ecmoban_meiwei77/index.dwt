<script>
    function addClass(id){
        var className=['new','sale','hots'];
        for(i=0;i<className.length;i++){
            var obj = document.getElementById(className[i]);
            var objMore = document.getElementById(className[i]+'More');
            if(className[i]==id){
                obj.className = ''
                objMore.className='';
            }else{
                obj.className = 'hide';
                objMore.className='hide';
            }
        }
    }
</script>
